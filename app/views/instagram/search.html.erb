<div class="container">
  <h1>Insta User Search</h1>



<div class="container">
  <%= form_tag(instagram_search_path, method: "get") do %>
    <p>
      <%= text_field_tag :search %>
      <%= submit_tag "Go", class: 'btn btn-default' %>
    </p>
  <% end %>
</div>


<%= form_tag instagram_search_path do %>
  <%= text_field_tag :search, params[:q], :placeholder => "Search for user" %>
  <%= submit_tag "Search" %>
<% end %>
