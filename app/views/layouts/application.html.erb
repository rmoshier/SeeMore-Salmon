<!DOCTYPE html>
<html>
<head>
  <title>Stream</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class = "container">

    <% if session[:user_id] %>
      <%= render "layouts/signedin" %>
    <% else %>
      <%= render "layouts/guest" %>
    <% end %>
  </div>

<%= yield %>





</body>
</html>
